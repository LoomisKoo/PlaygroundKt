<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.playgroundkt">
    <!-- 悬浮窗权限 -->
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.SYSTEM_OVERLAY_WINDOW" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PlaygroundKt">
        <activity android:name=".activity.SharePreferenceANRActivity"></activity>
        <activity android:name=".activity.kotlin.FlowActivity"></activity>
        <activity android:name=".activity.kotlin.KotlinActivity" />
        <activity android:name=".activity.kotlin.coroutine.AsyncActivity" />
        <activity android:name=".activity.FloatWindowsActivity" />
        <activity android:name=".activity.kotlin.coroutine.LaunchTypeActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>